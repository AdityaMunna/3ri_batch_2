{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Resources": {
        "myuser1": {
            "Type": "AWS::IAM::User",
            "Properties": {
                "Tags": [
                    {
                        "Key": "Environment",
                        "Value": "Development"
                    },
                    {
                        "Key": "Owner",
                        "Value": "apple"
                    }
                ],
                "UserName": "apple"
            }
        },
        "myuser2": {
            "Type": "AWS::IAM::User",
            "Properties": {
                "Tags": [
                    {
                        "Key": "Environment",
                        "Value": "stageing"
                    },
                    {
                        "Key": "Owner",
                        "Value": "apple-2"
                    }
                ],
                "UserName": "banana"
            }
        },
        "mygroup": {
            "Type": "AWS::IAM::Group",
            "Properties": {
                "GroupName": "group-1"
            }
        }
    }
}